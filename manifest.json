
{
  "manifest_version": 2,

  "name": "Date4uU",
  "version": "1.0",
  "description": "This extension gives the user the date of the article or webpage they are on.",

  "icons": {},
  "permissions": ["http://*.google.com/", "https://*.google.com/"],
  "browser_action": {
    "default_title": "Get date of web page",
    "default_icon": "date icon.png",
    "default_popup": ""
  }
}
